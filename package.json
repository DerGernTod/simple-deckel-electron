{
  "name": "simple-deckel-electron",
  "productName": "Simple Deckel",
  "description": "A sportsclub customer management tool",
  "main": "./public/index.js",
  "private": true,
  "author": "Gernot Raudner",
  "version": "2.0.0",
  "repository": "https://github.com/DerGernTod/simple-deckel-electron",
  "scripts": {
    "start": "brunch watch --server",
    "build": "brunch build --production",
    "electron": "electron ./public",
    "pack": "electron-packager ./ simple-deckel-electron --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=TheNode --version-string.FileDescription=TheNode --version-string.ProductName=\"Simple Deckel Electron\"",
    "pack-pi": "electron-packager ./ simple-deckel-electron --overwrite --asar=true --platform=linux --arch=armv7l --prune=true --out=release-builds --version-string.CompanyName=TheNode --version-string.FileDescription=TheNode --version-string.ProductName=\"Simple Deckel Electron\""
  },
  "dependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "auto-reload-brunch": "^2",
    "babel-brunch": "~6.0",
    "babel-preset-latest": "^6",
    "babel-preset-react": "~6.22",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "brunch": "^2",
    "clean-css-brunch": "^2",
    "dexie": "^3.2.2",
    "electron": "^5.0.1",
    "electron-debug": "^3.0.0",
    "electron-packager": "^14.0.0",
    "react-simple-keyboard": "^1.23.0",
    "redux-thunk": "^2.3.0",
    "simple-keyboard-layouts": "^1.5.2",
    "uglify-js-brunch": "^2"
  }
}
